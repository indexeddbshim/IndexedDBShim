{
  "name": "indexeddbshim",
  "version": "3.6.2",
  "author": "Parashuram <code@nparashuram.com>",
  "contributors": [
    "Brett Zamir"
  ],
  "homepage": "https://github.com/axemclion/IndexedDBShim",
  "description": "A polyfill for IndexedDB using WebSql",
  "scripts": {
    "start": "grunt",
    "dev": "grunt dev",
    "dev-browser": "grunt dev-browser",
    "dev-browserNoninvasive": "grunt dev-browserNoninvasive",
    "dev-node": "grunt dev-node",
    "dev-unicode": "grunt dev-unicode",
    "dev-unicodeNode": "grunt dev-unicodeNode",
    "build": "grunt build",
    "build-key": "grunt build-key",
    "build-browser": "grunt build-browser",
    "build-browserNoninvasive": "grunt build-browserNoninvasive",
    "build-node": "grunt build-node",
    "build-unicode": "grunt build-unicode",
    "build-unicodeNode": "grunt build-unicodeNode",
    "clean": "grunt clean",
    "clean-w3c": "grunt clean-w3c",
    "copy": "grunt -v copy",
    "connect-watch": "grunt connect-watch",
    "eslint": "grunt eslint",
    "sqlite-rebuild": "cd node_modules/sqlite3 && npm i",
    "w3c-build": "grunt clean-w3c && node test-support/node-buildjs",
    "test": "npm run sauce-qunit",
    "mocha": "grunt clean-mocha && grunt mocha && grunt clean-mocha",
    "sauce-qunit": "grunt sauce-qunit",
    "phantom-qunit": "grunt phantom-qunit",
    "node-qunit": "grunt clean-qunit && grunt nodequnit && grunt clean-qunit",
    "fake": "grunt clean-fake && grunt fake && grunt clean-fake",
    "mock": "grunt clean-mock && grunt mock && grunt clean-mock",
    "w3c-old": "grunt clean-w3c-old && grunt w3c-old && grunt clean-w3c-old",
    "tests-polyfill": "grunt clean-polyfill && npm run fake && npm run mock && npm run w3c-old && browserify-test -t [ babelify --presets es2015 ] tests-polyfill/**/*.js",
    "web-platform-tests": "cd web-platform-tests && ./wpt serve",
    "w3c": "npm run w3c-build && npm run w3c-test",
    "w3c-add-wrap": "node test-support/wrap-w3c-tests",
    "w3c-remove-wrap": "node test-support/wrap-w3c-tests remove",
    "w3c-wrap": "node test-support/wrap-w3c-tests",
    "w3c-test": "grunt clean-w3c && node test-support/node-idb-test && grunt clean-w3c",
    "w3c-good": "npm run w3c-build && node test-support/node-idb-test good && grunt clean-w3c",
    "w3c-bad": "npm run w3c-build && node test-support/node-idb-test bad && grunt clean-w3c",
    "w3c-good-test": "node test-support/node-idb-test good && grunt clean-w3c",
    "w3c-bad-test": "node test-support/node-idb-test bad && grunt clean-w3c"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/axemclion/IndexedDBShim.git"
  },
  "bugs": {
    "url": "https://github.com/axemclion/IndexedDBShim/issues"
  },
  "main": "dist/indexeddbshim-node.js",
  "browser": "dist/indexeddbshim.min.js",
  "module": "src/browser-noninvasive.js",
  "keywords": [
    "indexedDB",
    "database",
    "polyfill",
    "websql"
  ],
  "dependencies": {
    "babel-polyfill": "6.26.0",
    "eventtargeter": "0.4.0",
    "sync-promise": "git+https://github.com/brettz9/sync-promise.git#full-sync-missing-promise-features",
    "typeson": "5.8.2",
    "typeson-registry": "1.0.0-alpha.20",
    "websql": "git+https://github.com/brettz9/node-websql.git#configurable-secure"
  },
  "devDependencies": {
    "babel-core": "6.26.3",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-preset-env": "1.7.0",
    "babelify": "8.0.0",
    "bson": "2.0.6",
    "chai": "4.1.2",
    "colors": "1.2.5",
    "cyclonejs": "1.1.4",
    "eslint": "4.19.1",
    "eslint-config-standard": "11.0.0",
    "eslint-plugin-compat": "2.2.0",
    "eslint-plugin-import": "2.11.0",
    "eslint-plugin-node": "6.0.1",
    "eslint-plugin-promise": "3.7.0",
    "eslint-plugin-standard": "3.1.0",
    "grunt": "1.0.2",
    "grunt-browserify": "5.3.0",
    "grunt-cli": "1.2.0",
    "grunt-contrib-clean": "1.1.0",
    "grunt-contrib-connect": "1.0.2",
    "grunt-contrib-copy": "1.0.0",
    "grunt-contrib-qunit": "2.0.0",
    "grunt-contrib-uglify-es": "git://github.com/gruntjs/grunt-contrib-uglify.git#harmony",
    "grunt-contrib-watch": "1.1.0",
    "grunt-mocha-test": "0.13.3",
    "grunt-node-qunit": "git+https://github.com/brettz9/grunt-node-qunit.git#grunt1",
    "grunt-saucelabs": "git+https://github.com/brettz9/grunt-saucelabs.git#secure-saucelabs",
    "gruntify-eslint": "4.0.0",
    "is-date-object": "1.0.1",
    "isomorphic-fetch": "^2.2.1",
    "js-polyfills": "git+https://github.com/brettz9/polyfill.git#node",
    "jsdom": "11.10.0",
    "mapstraction": "git+https://github.com/brettz9/mapstraction.git#relativePaths",
    "mocha": "5.1.1",
    "sinon": "5.0.7",
    "source-map-support": "0.5.6",
    "sourcemap-transformer": "git+https://github.com/brettz9/sourcemap-transformer.git",
    "w3c-blob": "0.0.1",
    "ws": "5.1.1",
    "xmlhttprequest": "git+https://github.com/brettz9/node-XMLHttpRequest.git#local-and-prototype"
  },
  "bundleDependencies": [],
  "license": "(MIT OR Apache-2.0)",
  "engines": {
    "node": ">=6.9.2"
  }
}
